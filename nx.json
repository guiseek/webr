  },
  "projects": {
    "demo": { "tags": [] },
    "demo-e2e": { "tags": [], "implicitDependencies": ["demo"] },
    "workspace": { "tags": [] },
    "webr-forms": { "tags": ["type:ui"] },
    "webr-styles": { "tags": ["type:ui"] },
    "webr-icons": { "tags": ["type:ui"] }
  }
}
